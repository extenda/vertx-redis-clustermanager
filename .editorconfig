root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true
continuation_indent_size=4

[*.java]
indent_style=space
indent_size=2

[*.launch]
indent_style=space
indent_size=2

[{*.yml,*.yaml}]
indent_style=space
indent_size=2

[*.properties]
indent_style=space
indent_size=2
trim_trailing_whitespace=false